<p id="notice"><%= notice %></p>

<h1>Users</h1>
<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Password</th>
      <th colspan="3"></th>
    </tr>
  </thead>
    <%= render @users %>
  <tbody>
    <% @users.each do |user| %>
    <% if current_user.admin? && !current_user?(user) %>
    | <%= link_to "Delete", user, method: :delete,
                                  data: { confirm: "You sure?" } %>
                                  <% end %>
    <% end %>
  </tbody>
</table>
<%= will_paginate %>

<br>

<%= link_to 'New User', new_user_path %>
