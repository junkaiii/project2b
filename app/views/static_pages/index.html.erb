<% if logged_in? %>
<div>
<%= form_tag(jobs_path, :method => "get", id: "search-form") do %>
<%= text_field_tag :search, params[:search], placeholder: "Search Jobs" %>
<%= submit_tag "Search", :name => nil %>
<% end %>
</div>
<br>
<div>
<%= render @jobshow %>
<%= will_paginate @jobshow %>
</div>

<% else %>

<h1>Welcome!</h1>
<p>This will be an awesome index page!</p>
<p>This means you have not logged in!</p>

<% end %>
